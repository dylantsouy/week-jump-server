{
    "name": "srock-server",
    "version": "1.0.0",
    "description": "server for srock",
    "main": "index.js",
    "author": "Dylan Tsou",
    "license": "ISC",
    "scripts": {
        "build": "npx sequelize-cli db:migrate",
        "start": "node server.js",
        "deploy": "npx sequelize-cli db:migrate && node server.js",
        "dev": "nodemon server.js",
        "reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
        "seed": "npx sequelize-cli db:seed:all",
        "prepare": "husky install"
    },
    "engines": {
        "node": "16.x"
    },
    "dependencies": {
        "axios": "^1.6.8",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "bufferutil": "^4.0.7",
        "cheerio": "^1.0.0-rc.12",
        "cloudinary": "^1.41.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.19.2",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^2.3.3",
        "node-cron": "^3.0.3",
        "pg": "^8.13.3",
        "puppeteer": "^22.12.1",
        "read-excel-file": "^5.5.3",
        "sequelize": "^6.37.2",
        "socket.io": "^4.5.3",
        "utf-8-validate": "^5.0.10",
        "uuid": "^9.0.0",
        "ws": "^8.11.0"
    },
    "devDependencies": {
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^8.0.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "sequelize-cli": "^6.5.1"
    },
    "lint-staged": {
        "src/**/*.{js,ts,json}": [
            "prettier --write"
        ],
        "src/**/*.{js,ts}": [
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
